<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MusicSearch</display-name>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>searchProject.ServletHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ClusterServlet</servlet-name>
    <servlet-class>searchProject.ClusterServletHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ClusterServlet2</servlet-name>
    <servlet-class>searchProject.ClusterServletHandler2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ClusterServlet4</servlet-name>
    <servlet-class>searchProject.ClusterServletHandler4</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ClusterServlet3</servlet-name>
    <servlet-class>searchProject.ClusterServletHandler3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QexpServlet</servlet-name>
    <servlet-class>searchProject.QueryExpansionServletHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/MainServletPath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ClusterServlet</servlet-name>
    <url-pattern>/ClusterServletPath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ClusterServlet2</servlet-name>
    <url-pattern>/ClusterServletPath2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ClusterServlet3</servlet-name>
    <url-pattern>/ClusterServletPath3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ClusterServlet4</servlet-name>
    <url-pattern>/ClusterServletPath4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QexpServlet</servlet-name>
    <url-pattern>/QexpServletPath</url-pattern>
  </servlet-mapping>
</web-app>